{
    "manifest_version": 3,
    "name": "YouTube MrBeastify",
    "version": "1.7.0",
    "icons": {
        "96": "icon.png"
    },
    "description": "Modify YouTube thumbnails to include Morshu",
    "host_permissions": [
            "https://www.youtube.com/*"
    ],
    "content_scripts": [{
        "matches": ["https://www.youtube.com/*"],
        "js": ["Morshuify.js"],
        "run_at": "document_idle"
    }],
    "web_accessible_resources": [{
        "resources": [
            "images/*.png",
            "images/*.json"
        ],
        "matches": [
            "*://*/*"
        ]
    }]
}
