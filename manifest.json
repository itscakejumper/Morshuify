{
    "manifest_version": 2,
    "name": "Morshuify",
    "version": "0.1",
    "icons": {
        "96": "icon.png"
    },
    "description": "Modify YouTube thumbnails to include Morshu",
    "browser_action": {
    
    },
    "permissions": [
        "https://www.youtube.com/*"
    ],
    "content_scripts": [{
        "matches": ["https://www.youtube.com/*"],
        "js": ["morshuify.js"],
        "run_at": "document_idle"
    }],
    "web_accessible_resources": [
        "images/*.png",
        "images/*.json"
    ]
    
}
